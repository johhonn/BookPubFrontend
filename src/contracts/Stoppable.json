  "source": "pragma solidity ^0.4.15;\n\nimport \"./Owned.sol\";\n\ncontract Stoppable is Owned {\n\n  bool public running;\n\n  event LogRunSwitch(bool switchSetting);\n\n  modifier onlyIfRunning {\n    if(!running) revert();\n    _;\n  }\n\n  function Stoppable() {\n    running = true;\n  }\n\n  function runSwitch(bool onOff)\n    onlyOwner\n    returns(bool success){\n      running = onOff;\n      LogRunSwitch(onOff);\n      return true;\n    }\n}\n",
